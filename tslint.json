{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"max-classes-per-file": false,
		"max-line-length": [
			true,
			120
		],
		"no-console": false,
		"variable-name": [
			true,
			"check-format",
			"allow-leading-underscore",
			"allow-pascal-case"
		],
		"interface-name": [
			false,
			"always-prefix"
		]
	},
	"rulesDirectory": []
}